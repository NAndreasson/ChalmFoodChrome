{
  "manifest_version": 2,

  "name": "Chalmfood",
  "description": "Retrieves the dishes being served in Chalmers restaurants",
  "version": "1.0",

  "permissions": [
    "http://localhost:5000/aa"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "js": [ "jquery-1.9.1.min.js"],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}